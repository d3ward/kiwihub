{
  "name": "kiwhub",
  "version": "1.0.0",
  "description": "A curated collection of extensions, userscripts, guides, and actions for Kiwi Browser users, aimed at enhancing browsing experience",
  "scripts": {
    "dev": "webpack serve --config config/webpack.dev.js",
    "build": "webpack --config config/webpack.prod.js",
    "build-list": "node src/script/build_list.js",
    "format": "prettier --write '**/*.{js,ts}'",
    "purge": "node config/purgecss.mjs",
    "lint": "eslint  'src/**/*.{js,ts}' && echo 'Linting complete!'",
    "lint-fix": "eslint  'src/**/*.{js,ts}' --fix"
  },
  "author": "Eduard Ursu",
  "license": "(CC BY-NC-SA 4.0)",
  "bugs": {
    "url": "https://github.com/d3ward/kiwihub/issues"
  },
  "dependencies": {
    "a11y-dialog": "^8.0.4",
    "prismjs": "^1.29.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "babel-loader": "^9.1.3",
    "chalk": "^5.3.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.10.0",
    "ejs": "^3.1.9",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-webpack-plugin": "^4.1.0",
    "file-loader": "^6.2.0",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.8.1",
    "prettier": "^3.2.5",
    "purgecss": "^5.0.0",
    "sass": "^1.72.0",
    "sass-loader": "^14.1.1",
    "template-ejs-loader": "^0.9.4",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.3",
    "webpack-merge": "^5.10.0"
  }
}
